package arr2;

import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
	int n1 = 1;
	int n2 = 1;
	int n3;
	System.out.print("n1 = " + n1+"; " +"n2 = " + n2+" ");
	System.out.println("");
	System.out.print("Введіть кількість елементів з послідовності Фібоначчі: ");
	int b=input.nextInt();
	for(int i=0;i<b;i++) {
	n3=n1+n2;
	System.out.print(n3+" ");
	n1=n2;
	n2=n3;
	}
	
	}
	}

	
	
	
